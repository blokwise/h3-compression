{
  "name": "@blokwise/h3-compression",
  "type": "module",
  "version": "0.5.3",
  "packageManager": "pnpm@10.18.2",
  "description": "Adds compression to h3 request (brotli, gzip, deflate)",
  "author": {
    "name": "Julian Derungs",
    "email": "julian.derungs@deepbase.ch",
    "url": "https://github.com/aerophobic"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blokwise/h3-compression.git"
  },
  "keywords": [
    "h3",
    "compression",
    "h3-compression",
    "deflate",
    "gzip",
    "brotli",
    "zstandard"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./handler": {
      "types": "./dist/handler.d.mts",
      "import": "./dist/handler.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.mts"
      ],
      "handler": [
        "./dist/handler.d.mts"
      ]
    }
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev:prepare": "unbuild --stub && nuxi prepare .playground",
    "dev": "unbuild --stub && nuxi dev .playground",
    "test": "unbuild --stub && vitest",
    "test:bench": "unbuild --stub && vitest bench",
    "test:types": "unbuild --stub && vitest typecheck",
    "prepack": "unbuild",
    "release": "NPM_TOKEN=$BLOKWISE_RELEE_NPM_TOKEN RELEE_GH_TOKEN=$BLOKWISE_RELEE_GH_TOKEN relee"
  },
  "peerDependencies": {
    "h3": "^1.6.0 || ^2.0.0"
  },
  "dependencies": {
    "@antfu/utils": "^9.3.0",
    "defu": "^6.1.4",
    "scule": "^1.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "6.0.0",
    "@blokwise/relee": "^0.2.3",
    "@nuxt/devtools": "2.6.5",
    "@types/node": "24.9.2",
    "@vitest/coverage-v8": "4.0.6",
    "eslint": "9.38.0",
    "get-port-please": "^3.2.0",
    "h3": "1.15.4",
    "lint-staged": "16.2.6",
    "nuxi": "3.29.3",
    "nuxt": "^4.1.3",
    "ofetch": "^1.5.1",
    "simple-git-hooks": "2.13.1",
    "typescript": "5.9.3",
    "vitest": "4.0.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  }
}
