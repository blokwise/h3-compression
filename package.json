{
  "name": "@blokwise/h3-compression",
  "type": "module",
  "version": "0.3.3",
  "packageManager": "pnpm@10.18.2",
  "description": "Adds compression to h3 request (brotli, gzip, deflate)",
  "author": {
    "name": "Gregor Becker",
    "email": "gregor@codedredd.de"
  },
  "contributors": [
    {
      "name": "Julian Derungs",
      "email": "julian.derungs@deepbase.ch",
      "url": "https://github.com/aerophobic"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/blokwise/h3-compression#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blokwise/h3-compression.git"
  },
  "bugs": "https://github.com/blokwise/h3-compression/issues",
  "keywords": [
    "h3",
    "h3-compression",
    "gzip",
    "brotli",
    "deflate",
    "compression"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "test:types": "vitest typecheck",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "h3": "^1.6.0 || ^2.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "6.0.0",
    "@types/node": "24.9.1",
    "@types/supertest": "6.0.3",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.38.0",
    "h3": "1.15.4",
    "lint-staged": "16.2.5",
    "node-fetch-native": "1.6.7",
    "simple-git-hooks": "2.13.1",
    "supertest": "7.1.4",
    "typescript": "5.9.3",
    "unbuild": "3.6.1",
    "vitest": "3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
